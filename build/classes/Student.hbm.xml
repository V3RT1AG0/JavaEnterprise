<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="swehw3" table="student">
		<id name="id">
			<generator class="assigned"></generator>
		</id>

		<property name="firstname" type="java.lang.String"></property>
		<property name="lastname" type="java.lang.String"></property>

		<property name="address" type="java.lang.String"></property>
		<property name="city" type="java.lang.String"></property>

		<property name="state" type="java.lang.String"></property>
		<property name="zipcode" type="java.lang.String"></property>

		<property name="email" type="java.lang.String"></property>
		<property name="likeCampus" type="java.lang.String"></property>

		<property name="interestUniversity" type="java.lang.String"></property>
		<property name="recommend" type="java.lang.String"></property>

		<property name="comments" type="java.lang.String"></property>
		<property name="raffle" type="java.lang.String"></property>

		<property name="phoneNum" type="java.lang.String"></property>
		<property name="date" type="java.lang.String">
			<column name="formDate" />
		</property>

	</class>

</hibernate-mapping>